{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "flicknote-doc",
  "compatibility_date": "2025-06-07",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "assets": {
    "directory": "dist",
    "html_handling": "drop-trailing-slash",
    "not_found_handling": "404-page"
  },
  "routes": [
    {
      "pattern": "docs.flicknote.app/*",
      "zone_name": "flicknote.app"
    }
  ]
}
